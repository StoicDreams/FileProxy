<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.0.10</Version>
    <Authors>Erik Gassler</Authors>
    <Company>Stoic Dreams</Company>
    <Product>File Proxy</Product>
    <Description>Simple library to create a proxy service for a website to host files that are actually hosted in a different location and/or domain.</Description>
    <Copyright>Copyright (c) 2019 Stoic Dreams</Copyright>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/StoicDreams/FileProxy</PackageProjectUrl>
    <RepositoryUrl>https://github.com/StoicDreams/FileProxy</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageReleaseNotes>Alpha. Project is functional but still in early development and testing.</PackageReleaseNotes>
    <ApplicationIcon>Logo.ico</ApplicationIcon>
    <PackageIconUrl>https://www.myfi.ws/img/sd/logoT600.png</PackageIconUrl>
    <AssemblyVersion>0.0.10.0</AssemblyVersion>
    <FileVersion>0.0.10.0</FileVersion>
    <PackageTags>Stoic Dreams,Proxy</PackageTags>
    <RootNamespace>StoicDreams.FileProxy</RootNamespace>
    <AssemblyName>StoicDreams.FileProxy</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\Docs\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Hosting.Abstractions" Version="2.2.0" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="2.2.0" />
  </ItemGroup>

</Project>
